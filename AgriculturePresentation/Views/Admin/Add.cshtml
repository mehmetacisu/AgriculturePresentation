@model EntityLayer.Concrete.Admin

@{
    ViewData["Title"] = "Add";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<form method="post" class="forms-sample">
	<div class="card">
		<div class="card-body">
			<h4 class="card-title text-center">Yeni Admin Girişi</h4>
			<div class="form-group">
				<label >Kullanıcı Adı</label>
				<input type="text" class="form-control" id="Title" asp-for="UserName">
			</div>
					<div class="form-group">
				<label >Şifre</label>
				<input type="password" class="form-control" id="Title" asp-for="Password">
			</div>
			<button type="submit" class="btn btn-success me-2">Kaydet</button>
		</div>
	</div>
</form>

